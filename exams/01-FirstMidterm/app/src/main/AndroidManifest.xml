<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="dev.beriashvili.exams.firstmidterm">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_spacex_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_spacex_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup">
        <activity
            android:name=".activities.QueryParametersActivity"
            android:label="Query Parameters" />
        <activity android:name=".activities.AuthenticationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LaunchActivity" />
        <activity android:name=".activities.LaunchesActivity" />
    </application>

</manifest>